{
    "compilerOptions": {
        "outDir": "./apps/public/js/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "baseUrl": ".", // This must be specified if "paths" is.
        "paths": {
            "jquery": ["node_modules/jquery/dist/jquery"] // This mapping is relative to "baseUrl"
        }
    },
    "files": [
        "./qbetable.tsx"
    ],
    "include": [
        "./src/**/*"
    ],
"exclude": [
        "node_modules",
        "node_modules/@types/core-js/*",
        "**/*.spec.ts"
    ]
}